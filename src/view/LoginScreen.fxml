<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginScreenController">
   <children>
      <TextField fx:id="username_field" layoutX="76.0" layoutY="99.0" promptText="Username" />
      <PasswordField fx:id="password_field" layoutX="76.0" layoutY="150.0" promptText="Password" />
      <Button fx:id="login_button" layoutX="127.0" layoutY="198.0" mnemonicParsing="false" onAction="#LoginButtonHandler" text="Login" />
   </children>
</AnchorPane>
