   <children>
      <Button fx:id="login_button" layoutX="105.0" layoutY="246.0" mnemonicParsing="false" onAction="#LoginButtonHandler" text="Login" />
      <PasswordField fx:id="password_field" layoutX="105.0" layoutY="185.0" promptText="Password" />
      <TextField fx:id="username_field" layoutX="105.0" layoutY="131.0" promptText="Username" />
   </children>
</AnchorPane>
